@model SalesWebMVC.Models.Seller

@{
    ViewData["Title"] = "Create";
}

<h2>@ViewData["title"]</h2>

<h4>Seller</h4>
<hr />
<div class="row">
    <div class=" col-md-4">
        <form asp-action="Create">
            <div class=" form-group">
                <label asp-for="Name" class="custom-label"></label>
                <input asp-for="Name" class="form-control" />
            </div>
            <div class=" form-group">
                <label asp-for="Email" class="custom-label"></label>
                <input asp-for="Email" class="form-control" />
            </div>
            <div class=" form-group">
                <label asp-for="birthDate" class="custom-label"></label>
                <input asp-for="birthDate" class="form-control" />
            </div>
            <div class=" form-group">
                <label asp-for="baseSalary" class="custom-label"></label>
                <input asp-for="baseSalary" class="form-control" />
            </div>
            <div class=" form-group">
                 <input type="submit" value="Create" class="btn btn-dark" />
            </div>

        </form>
    </div>
</div>

